.hero-home-17 {
    background-size: cover;
    background-position: center;
}
.hero-home-17 .mdp-data-hero-home-first {
    width: 100% !important;
}
.hero-home-17 .mdp-data-hero-home-first p {
    margin: 0 !important;
    margin-top: 8px !important;
    font-family: {{ settings.weare_font_text }} !important;
    font-style: normal;
    font-weight: 600;
    font-size: 14px !important;
    line-height: 24px !important;
    letter-spacing: 0.4px !important;
}
.hero-home-17 .mdp-data {
    display: flex;
    position: relative;
    width: 40%;
    padding: 60px;
    box-sizing: border-box;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-title {
    font-family: {{ settings.weare_font_header }};
    font-style: normal;
    font-weight: bold;
    font-size: 38px;
    line-height: 39px;
    color: #343434;
    text-align: center;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-subtitle {
    margin-bottom: 5px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 22px;
    letter-spacing: 0.133333px;
    text-transform: uppercase;
    color: #343434;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-button {
    font-size: 15px;
    letter-spacing: 0.5px;
    display: inline-flex;
    white-space: nowrap;
    align-items: center;
    color: #2a2a2a;
    border-bottom: 1px solid transparent;
    position: relative;
    font-weight: normal;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-button .mdp-text {
    margin-right: 5px;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-button .mdp-icon {
    font-size: 0;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-button .mdp-icon svg {
    width: 20px;
    height: 20px;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-button::before {
    content: "";
    position: absolute;
    transition: 0.3s all ease-in-out;
    left: 0;
    right: 0;
    bottom: -4px;
    height: 0;
    border-bottom: 1px solid #2a2a2a;
    opacity: 0;
    transform: translateY(5px);
}
.hero-home-17 .mdp-data > .mdp-header .mdp-button:hover::before {
    opacity: 1;
    transform: translateY(0);
}
.hero-home-17 .mdp-data > .mdp-header .mdp-row-full {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}
.hero-home-17 .mdp-data > .mdp-header .mdp-row-full .mdp-row-left {
    margin-right: 30px;
}
.hero-home-17 .mdp-data .mdp-box-content {
    width: 35%;
    max-width: 35%;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-header .mdp-title {
    font-family: {{ settings.weare_font_header }};
    font-style: normal;
    font-weight: bold;
    font-size: 48px;
    line-height: 62px;
    letter-spacing: 0.15px;
    color: #FFFFFF;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-header .mdp-title-hero-home-first,
.hero-home-17 .mdp-data .mdp-box-content .mdp-header .mdp-subtitle {
    font-family: '{{ settings.weare_font_header }}';
    font-style: normal;
    font-weight: 700;
    font-size: 48px;
    line-height: 65px;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: {{ settings.mdp_header_color }};
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-header .mdp-subtitle {
    color: #599E8F;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-header .mdp-subtitle span {
    font-size: 32px;
}
.hero-home-17 .mdp-data-hero-home-first .mdp-box-content .mdp-hero-home-first-text p {
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    font-size: 14px;
    line-height: 24px;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-hero-home-first-text ul {
    padding: 0;
    margin: 37px 0 0;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-hero-home-first-text ul li {
    padding: 0;
    margin-top: 0;
    margin-bottom: 14px;
    list-style: none;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-hero-home-first-text ul li .mdp-icon {
    color: #F9F9F9;
    margin-right: 12px;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-hero-home-first-text ul li p {
    display: inline-block;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 400;
    font-size: 16px;
    line-height: 26px;
    letter-spacing: 0.2px;
    color: #F9F9F9;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-content {
    margin-top: 30px;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-content p:first-child {
    margin-top: 0;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-content p:last-child {
    margin-bottom: 0;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-content a {
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-content a:hover {
    color: #5838e9;
    transform: scale(1.5);
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-footer .mdp-button {
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    font-size: 14px;
    line-height: 24px;
    letter-spacing: 0.4px;
    text-decoration-line: underline;
    color: {{ settings.link_color }};
    margin-top: 20px;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-footer .mdp-button:hover{
    color: {{ settings.link_hover_color }};
    transition: color 500ms;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-footer .mdp-button::before {
    content: none;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-signup-form {
    margin-top: 30px;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-signup-form .mdp-row input {
    display: inline-block;
    width: 340px;
    max-height: 50px;
    background: #FFFFFF;
    border: 1px solid #979797;
    box-sizing: border-box;
    border-radius: 2px;
    padding: 12px 17px;
    text-align: left;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 26px;
    letter-spacing: 0.2px;
    color: #7B7B7B;
}
.hero-home-17 .mdp-data .mdp-box-content .mdp-signup-form .mdp-row .mdp-submit {
    display: inline-block;
    width: 140px;
    max-height: 50px;
    margin-left: 5px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 14px;
    line-height: 19px;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: #FFFFFF;
    background: #343434;
    border-radius: 2px;
    padding: 16px 18px;
}
.hero-home-17 .mdp-data .mdp-box-content div p {
    font-family: {{ settings.weare_font_text }};
    margin: 0;
    font-size: 16px;
    line-height: 26px;
    letter-spacing: 0.2px;
}
.hero-home-17.mdp-inverted .mdp-data .mdp-header .mdp-title {
    color: white;
}
.hero-home-17.mdp-inverted .mdp-data .mdp-header .mdp-subtitle {
    color: white;
}
.hero-home-17.mdp-inverted .mdp-data .mdp-content {
    color: white;
}
.hero-home-17.mdp-inverted .mdp-data .mdp-footer .mdp-button {
    padding: 9px 15px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    border-radius: 0;
    background-image: none !important;
    font-size: 14px;
    background-color: white;
    border-color: white;
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.hero-home-17.mdp-inverted .mdp-data .mdp-footer .mdp-button:hover {
    background-color: #5838e9;
    border-color: #5838e9;
    color: white;
}
.hero-home-17.mdp-backed .mdp-data {
    background-color: white;
}
.hero-home-17.mdp-backed.mdp-inverted .mdp-data {
    background-color: #2a2a2a;
}
.hero-home-17:not(.mdp-backed) .mdp-data {
    padding: 0;
}
.hero-home-17[data-variant=right_text] .mdp-data {
    margin-left: auto;
}
.hero-home-17[data-variant=right_text]:not(.mdp-backed) .mdp-data {
    text-align: right;
}
.hero-home-17[data-variant=center_text] .mdp-data {
    width: 65%;
    margin-left: auto;
    margin-right: auto;
    text-align: center;
}
.hero-home-17 .mdp-data-hero-home-first .mdp-box-content .mdp-hero-home-first-text p{
    color: {{ settings.mdp_paragraph_color }};
}

