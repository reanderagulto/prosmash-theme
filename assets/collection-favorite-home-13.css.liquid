.collection-favorite-home-13{
    margin-top: 0;
    margin-bottom: 0;
}

.mdp-stretch-height{
    width: 100%;
    display: flex;
    align-items: stretch;
    flex-wrap: wrap;
    justify-content: space-between;
}

.collection-favorite-home-13 > .mdp-header .mdp-title {
    font-family: {{ settings.weare_font_header }};
    font-style: normal;
    font-weight: bold;
    font-size: 38px;
    line-height: 39px;
    color: #343434;
    text-align: center;
}

.collection-favorite-home-13 > .mdp-header .mdp-subtitle {
    margin-bottom: 5px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 22px;
    letter-spacing: 0.133333px;
    text-transform: uppercase;
    color: #343434;
}

.collection-favorite-home-13 > .mdp-header .mdp-button{
    font-size: 15px;
    letter-spacing: 0.5px;
    display: inline-flex;
    white-space: nowrap;
    align-items: center;
    color: #2a2a2a;
    border-bottom: 1px solid transparent;
    position: relative;
    font-weight: normal;
}

.collection-favorite-home-13 > .mdp-header .mdp-button .mdp-text {
    margin-right: 5px;
}

.collection-favorite-home-13 > .mdp-header .mdp-button .mdp-icon {
    font-size: 0;
}

.collection-favorite-home-13 > .mdp-header .mdp-button .mdp-icon svg{
    width: 20px;
    height: 20px;
}

.collection-favorite-home-13 > .mdp-header .mdp-button::before {
    content: "";
    position: absolute;
    transition: 0.3s all ease-in-out;
    left: 0;
    right: 0;
    bottom: -4px;
    height: 0;
    border-bottom: 1px solid #2a2a2a;
    opacity: 0;
    transform: translateY(5px);
}

.collection-favorite-home-13 > .mdp-header .mdp-button:hover::before {
    opacity: 1;
    transform: translateY(0);
}

.collection-favorite-home-13 > .mdp-header .mdp-row-full {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}

.collection-favorite-home-13 > .mdp-header .mdp-row-full .mdp-row-left {
    margin-right: 30px;
}
@media (max-width: 600px) {

    .collection-favorite-home-13 > .mdp-header .mdp-row-full {
        display: block;
    }

    .collection-favorite-home-13 > .mdp-header .mdp-row-full .mdp-row-left {
        justify-content: center;
        padding: 10px 10px 5px 10px;
        margin-right: 0;
    }

    .collection-favorite-home-13 > .mdp-header .mdp-row-full .mdp-row-right {
        justify-content: center;
        padding: 5px 10px 10px 10px;
    }
}

.collection-favorite-home-13 > .mdp-header {
    margin-bottom: 40px;
}

.collection-favorite-home-13 .mdp-box-btn {
    margin-top: 30px;
    text-align: center;
}
.collection-favorite-home-13 .mdp-box-btn a {
    display: inline-block;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 700;
    font-size: 14px;
    line-height: 19px;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: #343434;
    padding: 15px 75px;
    border: 1px solid #7B7B7B;
    box-sizing: border-box;
    border-radius: 2px;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media{
    position: relative;
}

.mdp-flex-center{
    display: flex;
    align-items: center;
    justify-content: center;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media a {
    display: flex;
    height: 100%;
}
.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-text-content{
    position: absolute;
    bottom: 45px;
    z-index: 1;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-text-content .mdp-box-bg{
    text-align: center;
    padding: 40px 80px;
    background-color: rgba(0, 0, 0, 0.5);
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-text-content .mdp-box-bg h3{
    font-family: '{{ settings.weare_font_header }}';
    font-style: normal;
    font-weight: 700;
    font-size: 32px;
    line-height: 40px;
    text-align: center;
    color: white;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-text-content .mdp-box-bg .mdp-btn-product{
    display: inline-block;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    font-size: 14px;
    line-height: 24px;
    letter-spacing: 0.4px;
    text-decoration-line: underline;
    color: white;
    margin-top: 10px;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-text-content .mdp-box-bg .mdp-btn-product:hover{
   color: {{ settings.link_hover_color }};
   transition: color 300ms;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media a img{
    width: 100%;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-btn-product {
    text-align: center;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-btn-product span {
    padding: 10px 40px;
    border-radius: 2px;
    background: #FFFFFF;
    border-bottom: 3px solid #343434;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 700;
    font-size: 12px;
    line-height: 16px;
    text-align: center;
    letter-spacing: 0.342857px;
    text-transform: uppercase;
    color: #343434;
}
.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-box-collection {
    position: absolute;
    z-index: 999;
    left: 0;
    right: 0;
    bottom: 55px;
    text-align: center;
    mix-blend-mode: normal;
    opacity: 0.8;
}
.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-box-collection .mdp-collection-name {
    font-family: {{ settings.weare_font_header }};
    font-style: normal;
    font-weight: 700;
    font-size: 32px;
    line-height: 40px;
    text-align: center;
    color: #343434;
}
.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-box-collection .mdp-link-collection {
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 700;
    font-size: 14px;
    line-height: 19px;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: #343434;
    border: 1px solid #7B7B7B;
    padding: 15px 65px;
    border-radius: 2px;
    display: inline-block;
    margin-top: 35px;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-swiper-container .mdp-swiper-wrapper .mdp-swiper-slide .mdp-image{
    min-width: 100%;
    max-width: initial;
}
.collection-favorite-home-13 .mdp-favorite .mdp-big-container {
    width: calc(60% - 30px);
    max-height: 100%;
    margin-right: 30px;
    overflow: hidden;
}
.collection-favorite-home-13 .mdp-favorite .mdp-small-container{
    width: 40%;
    max-height: 100%;
    overflow: hidden;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-swiper-container .mdp-swiper-wrapper .mdp-swiper-slide .mdp-product-title{
    visibility: hidden;
    opacity: 0;
    transition: visibility 0.3s linear,opacity 0.3s linear;
    position: absolute;
    bottom: 30px;
    left: 30px;
    right: 30px;
    z-index: 9;
    font-family: '{{ settings.weare_font_header }}';
    font-style: normal;
    font-weight: 700;
    font-size: 32px;
    line-height: 1.2em;
    color: white;
    background-color: rgba(0, 0, 0, 0.5);
    padding: 20px;
}

.collection-favorite-home-13 .mdp-favorite .mdp-media .mdp-swiper-container .mdp-swiper-wrapper .mdp-swiper-slide:hover .mdp-product-title{
    visibility: visible;
    opacity: 1;
}

