}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-content {
    margin-top: 30px;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-content p:first-child {
    margin-top: 0;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-content p:last-child {
