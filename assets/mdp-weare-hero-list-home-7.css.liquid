}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-content a {
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-content a:hover {
    color: #5838e9;
    transform: scale(1.5);
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-footer .mdp-button {
    margin-top: 23px;
    margin-left: 60px;
    border-radius: 0;
    background-image: none !important;
    cursor: pointer;
    width: auto;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    padding: 15px 45px;
    font-size: 14px;
    line-height: 19px;
    background-color: #343434;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    border-color: #4c90c7;
    color: white;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-footer .mdp-button:hover {
    background-color: white;
    color: #2a2a2a;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-footer .mdp-button::before {
    content: none;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-signup-form {
