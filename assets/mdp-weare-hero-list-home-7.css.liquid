/* START hero home 7 */
.mdp-weare-hero-list-home-7 {
    background-size: cover;
    background-position: center;
    padding-top: 100px;
    padding-bottom: 100px;
}
.mdp-weare-hero-list-home-7 .mdp-data-hero-home-7 {
    width: 100% !important;
}
.mdp-weare-hero-list-home-7 .mdp-data-hero-home-7 p {
    max-width: 640px;
    font-family: 'Open Sans';
    font-style: normal;
    font-weight: 700;
    font-size: 16px;
    line-height: 1.6em;
    letter-spacing: 0.4px;
    color: white;
}
.mdp-weare-hero-list-home-7 .mdp-data {
    display: flex;
    position: relative;
    width: 40%;
    padding: 60px;
    box-sizing: border-box;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-title {
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    color: {{ settings.mdp_header_color }};
    text-align: center;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-subtitle {
    margin-bottom: 5px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 22px;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: #343434;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-button {
    font-size: 15px;
    letter-spacing: 0.5px;
    display: inline-flex;
    white-space: nowrap;
    align-items: center;
    color: #2a2a2a;
    border-bottom: 1px solid transparent;
    position: relative;
    font-weight: normal;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-button .mdp-text {
    margin-right: 5px;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-button .mdp-icon {
    font-size: 0;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-button .mdp-icon svg {
    width: 20px;
    height: 20px;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-button::before {
    content: "";
    position: absolute;
    transition: 0.3s all ease-in-out;
    left: 0;
    right: 0;
    bottom: -4px;
    height: 0;
    border-bottom: 1px solid #2a2a2a;
    opacity: 0;
    transform: translateY(5px);
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-button:hover::before {
    opacity: 1;
    transform: translateY(0);
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-row-full {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}
.mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-row-full .mdp-row-left {
    margin-right: 30px;
}
@media (max-width: 600px) {
    .mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-row-full {
        display: block;
    }
    .mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-row-full .mdp-row-left {
        justify-content: center;
        padding: 10px 10px 5px 10px;
        margin-right: 0;
    }
    .mdp-weare-hero-list-home-7 .mdp-data > .mdp-header .mdp-row-full .mdp-row-right {
        justify-content: center;
        padding: 5px 10px 10px 10px;
    }
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list {
    width: calc(50% - 194px);
    max-width: calc(50% - 194px);
    padding: 0 97px;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-header .mdp-title {
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    letter-spacing: 0.4px;
    color: #FFFFFF;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-header .mdp-title-hero-home-7,
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-header .mdp-subtitle {
    max-width: 640px;
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: 700;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    letter-spacing: 0.4px;
