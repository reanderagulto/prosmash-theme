}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-hero-home-7-text ul li .mdp-icon {
color: {{ settings.mdp_paragraph_color }};
margin-right: 12px;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-hero-home-7-text ul li p {
    display: inline-block;
    font-family: "{{ settings.weare_font_text }}";
    font-style: normal;
    font-weight: 400;
    font-size: {{ settings.mdp_paragraph_font_size }}px;
