}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-signup-form .mdp-row input {
    display: inline-block;
    width: 340px;
    max-height: 50px;
    background: #FFFFFF;
    border: 1px solid #979797;
    box-sizing: border-box;
    border-radius: 2px;
    padding: 12px 17px;
    text-align: left;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 26px;
    letter-spacing: 0.2px;
    color: #7B7B7B;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list .mdp-signup-form .mdp-row .mdp-submit {
    display: inline-block;
    width: 140px;
    max-height: 50px;
    margin-left: 5px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 14px;
    line-height: 19px;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: #FFFFFF;
    background: #343434;
    border-radius: 2px;
    padding: 16px 18px;
}
.mdp-weare-hero-list-home-7 .mdp-data .mdp-box-list div p {
    font-family: {{ settings.weare_font_text }};
    margin: 0;
    font-size: 16px;
    line-height: 26px;
    letter-spacing: 0.2px;
}
