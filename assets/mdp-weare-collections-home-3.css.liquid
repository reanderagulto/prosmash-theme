.mdp-weare-collections-home-3 {
    margin: 80px;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection {
    position: relative;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-media .mdp-placeholder {
    background-color: transparent;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-media .mdp-image {
    width: 100%;
    margin: auto;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-content-box {
    display: none;
    position: absolute;
    background-color: rgba(255, 255, 255, 0.9);
    width: 60%;
    z-index: 9;
    left: 20%;
    top: 24px;
    bottom: 24px;
    transition: 0.3s all ease-in-out;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-content-box ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-content-box ul li {
    margin-bottom: 10px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    font-size: 14px;
    line-height: 24px;
    text-align: center;
    color: #7B7B7B;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-content-box ul li a {
    transition: 0.3s all ease-in-out;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-content-box ul li a:hover {
    transform: scale(1.5);
    color: #343434;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-content-box .mdp-line-box {
    z-index: 99;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: #343434;
    width: 100%;
    height: 3px;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-button-box {
    z-index: 1;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    flex-wrap: wrap;
    text-align: center;
    justify-content: center;
    align-items: center;
    display: flex;
    background: linear-gradient(180deg, rgba(0, 0, 0, 0.0287651) -48.19%, rgba(0, 0, 0, 0.558741) 100%);
    height: auto;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-button-box .mdp-button-shop {
    width: 120px;
    height: fit-content;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 14px;
    line-height: 19px;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: #FFFFFF;
    display: inline-block;
    padding: 15px 40px;
    border-radius: 2px;
    border: 2px solid #FFFFFF;
    margin-top: 20px;
    margin-bottom: 70px;
    transition: background 0.3s, border 0.3s;
}

.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-button-box .mdp-button-shop:hover {
    background: {{ settings.btn_hover_background }};
    border: 2px solid {{ settings.btn_hover_background }};
    color: {{ settings.btn_hover_color }};
}

.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-button-box .mdp-content-box-header {
    width: 100%;
    margin-top: 71px;
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: 32px;
    line-height: {{ settings.mdp_header_line_height }}em;
    text-align: center;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_header_color }};
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-data {
    position: absolute;
    left: 30px;
    right: 30px;
    top: 50%;
    transform: translateY(-50%);
    text-align: center;
    padding: 30px;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-data .mdp-header .mdp-title {
    font-size: {{ settings.weare_font_header }}px;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-data .mdp-content {
    margin-top: 10px;
    overflow-wrap: break-word;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-data .mdp-footer .mdp-button-shop {
    margin-top: 20px;
    border-radius: 0;
    background-image: none !important;
    cursor: pointer;
    width: auto;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    padding: 15px 45px;
    font-size: 14px;
    line-height: 19px;
    background-color: #343434;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    border-color: #4c90c7;
    color: white;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-data .mdp-footer .mdp-button-shop:hover {
    background-color: white;
    color: #2a2a2a;
}
.mdp-weare-collections-home-3 > .mdp-grid > .mdp-collection .mdp-data .mdp-footer .mdp-button-shop::before {
    content: none;
}
.mdp-weare-collections-home-3[data-variant=style_2] > .mdp-grid > .mdp-collection .mdp-media {
    background-color: #2a2a2a;
}
.mdp-weare-collections-home-3[data-variant=style_2] > .mdp-grid > .mdp-collection .mdp-data .mdp-header .mdp-title {
    color: white;
}
.mdp-weare-collections-home-3[data-variant=style_2] > .mdp-grid > .mdp-collection .mdp-data .mdp-content {
    color: white;
}
.mdp-weare-collections-home-3[data-variant=style_2] > .mdp-grid > .mdp-collection .mdp-data .mdp-footer .mdp-button-shop {
    margin-top: 20px;
    text-transform: uppercase;
    padding: 9px 15px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    border-radius: 0;
    background-image: none !important;
    font-size: 14px;
    background-color: white;
    border-color: white;
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-collections-home-3[data-variant=style_2] > .mdp-grid > .mdp-collection .mdp-data .mdp-footer .mdp-button-shop:hover {
    background-color: #5838e9;
    border-color: #5838e9;
    color: white;
}

