.mdp-grid[data-columns][data-gap] {
    width: 100%;
    display: flex;
    align-items: stretch;
    flex-wrap: wrap;
}

.mdp-grid[data-columns][data-gap] > div {
    box-sizing: border-box;
    width: 100%;
}

.mdp-columns[data-columns][data-gap] {
    break-inside: avoid-column;
}

.mdp-columns[data-columns][data-gap] > div {
    box-sizing: border-box;
    break-inside: avoid-column;
    overflow: hidden;
    display: inline-block;
}

.mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(odd){
    margin-left: 0;
    width: calc(50% - 15px);
}

.mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(even){
    margin-right: 0;
    width: calc(50% - 15px);
}

@media (min-width: 376px) and (max-width: 425px) {
    .mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(odd),
    .mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(even){
        margin: 0;
        width: 100%;
    }
}

@media (min-width: 321px) and (max-width: 375px) {
    .mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(odd),
    .mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(even){
        margin: 0;
        width: 100%;
    }
}

@media (max-width: 320px) {
    .mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(odd),
    .mdp-grid[data-columns][data-gap] >.mdp-grid[data-columns][data-gap] > div:nth-child(even){
        margin: 0;
        width: 100%;
    }
}

