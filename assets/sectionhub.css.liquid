 */
.mdp-grid[data-columns][data-gap][data-columns="1"] > div:not(:last-child) {
    margin-bottom: 15px !important;
}

.mdp-grid[data-columns][data-gap][data-columns="1"][data-gap="0"] > div {
    width: 100%;
}

.mdp-grid[data-columns][data-gap][data-columns="1"][data-gap="5"] > div {
