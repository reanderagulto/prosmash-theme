/* START slideshow top home 11 */
.slideshow-top-home-11 .mdp-slide {
    position: relative;
}
.slideshow-top-home-11 .mdp-slide .mdp-media .mdp-img-box img{
    width: 100%;
    height: 800px;
}
.slideshow-top-home-11 .mdp-slide .mdp-data {
    width: 100%;
    margin-top: 25px;
    text-align: center;
}
.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-header .mdp-title {
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    text-align: center;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_header_color }};
}
.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-content {
    margin-top: 15px;
    font-size: 20px;
}
.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-content p:first-child {
    margin-top: 0;
}
.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-content p:last-child {
    margin-bottom: 0;
}
.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-content a {
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-content a:hover {
    color: #5838e9;
    transform: scale(1.5);
}
.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-footer .mdp-button {
    margin-top: 10px;
    font-family: var(--font-body-family);
    font-style: normal;
    font-weight: 600;
    font-size: 14px;
    line-height: 24px;
    letter-spacing: 0.4px;
    color: {{ settings.link_color }};
}

.slideshow-top-home-11 .mdp-slide .mdp-data .mdp-footer .mdp-button:hover {
    color: {{ settings.link_hover_color }};
}

.slideshow-top-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-header .mdp-title {
    color: white;
}
.slideshow-top-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-content {
    color: white;
}
.slideshow-top-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-footer .mdp-button {
    padding: 9px 15px;
    font-family: var(--font-body-family);
    font-style: normal;
    font-weight: 600;
    border-radius: 0;
    background-image: none !important;
    font-size: 14px;
    background-color: white;
    border-color: white;
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}

.slideshow-top-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-footer .mdp-button:hover {
    background-color: #5838e9;
    border-color: #5838e9;
    color: white;
}
.slideshow-top-home-11 .mdp-swiper-pagination {
    position: initial;
    margin-top: 10px;
}

.swiper-button-prev-slideshow-top-home-11:after,
.swiper-button-next-slideshow-top-home-11:after{
    color: white;
}

.swiper-button-prev-slideshow-top-home-11:hover:after,
.swiper-button-next-slideshow-top-home-11:hover:after{
    color: #E9384F;
}

