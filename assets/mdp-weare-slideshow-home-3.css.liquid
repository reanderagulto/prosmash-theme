.mdp-weare-slideshow-home-3 .mdp-swiper-container {
    margin-left: 80px;
    margin-right: 80px;
}
.mdp-weare-slideshow-home-3 .mdp-slide {
    position: relative;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-media {
    background-repeat: no-repeat;
    background-size: contain;
    background-position: left;
    height: 723px;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-min-media {
    position: absolute;
    top: 136px;
    right: 0;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data {
    position: absolute;
    left: 55%;
    top: 50%;
    transform: translate(-30%, -50%);
    max-width: 600px;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-header .mdp-subtitle {
    margin-top: 0;
    margin-bottom: 8px;
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 22px;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: {{ settings.mdp_header_color }};
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-header .mdp-title {
    text-align: left;
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_header_color }};
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-content {
    margin-top: 15px;
    font-size: 20px;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-content p:first-child {
    margin-top: 0;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-content p:last-child {
    margin-bottom: 0;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-content a {
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-content a:hover {
    color: #5838e9;
    transform: scale(1.5);
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-footer .mdp-button {
    margin-top: 27px;
    cursor: pointer;
    width: auto;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    padding: 15px 45px;
    font-size: 14px;
    line-height: 19px;
    background-color: {{ settings.btn_background }};
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: {{ settings.btn_color }};
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
    border-radius: 2px;
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-footer .mdp-button:hover {
    background-color: {{ settings.btn_hover_background }};
    color: {{ settings.btn_hover_color }};
}
.mdp-weare-slideshow-home-3 .mdp-slide .mdp-data .mdp-footer .mdp-button::before {
    content: none;
}
.mdp-weare-slideshow-home-3 .mdp-slide.mdp-inverted .mdp-data .mdp-header .mdp-title {
    color: white;
}
.mdp-weare-slideshow-home-3 .mdp-slide.mdp-inverted .mdp-data .mdp-content {
    color: white;
}
.mdp-weare-slideshow-home-3 .mdp-slide.mdp-inverted .mdp-data .mdp-footer .mdp-button {
    padding: 9px 15px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    border-radius: 0;
    background-image: none !important;
    font-size: 14px;
    background-color: white;
    border-color: white;
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-slideshow-home-3 .mdp-slide.mdp-inverted .mdp-data .mdp-footer .mdp-button:hover {
    background-color: #5838e9;
    border-color: #5838e9;
    color: white;
}

