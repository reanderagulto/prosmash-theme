.mdp-weare-call-to-action-shop .mdp-data {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    margin-left: 95px !important;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-call-box {
    max-width: 440px;
    max-height: 232px;
    padding: 26px 33px;
    background: rgba(255, 255, 255, 0.654373);
    text-align: left;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-call-box .mdp-header .mdp-title {
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_header_color }} !important;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-call-box .mdp-content {
    margin: 0;
    font-family: "{{ settings.weare_font_text }}";
    font-style: normal;
    font-weight: 600;
    font-size: {{ settings.mdp_paragraph_font_size }}px;
    line-height: {{ settings.mdp_paragraph_line_height }}em;
    color: {{ settings.mdp_paragraph_color }};
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-call-box .mdp-content p {
    width: 100%;
    text-align: left;
    color: {{ settings.mdp_paragraph_color }};
}
.mdp-weare-call-to-action-shop{
    text-align: center;
}
.mdp-weare-call-to-action-shop > div{
    position: relative;
    z-index: 0;
    padding-top: 100px;
    padding-bottom: 100px;
}
.mdp-weare-call-to-action-shop > div > .mdp-overlay{
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-position: center;
    background-size: cover;
    pointer-events: none;
    z-index: -1;
}
.mdp-weare-call-to-action-shop .mdp-data{
    position: relative;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-header .mdp-title{
    font-size: {{ settings.mdp_header_font_size }}px;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-content{
    margin-top: 15px;
    font-size: 20px;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-content p:first-child{
    margin-top: 0;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-content p:last-child{
    margin-bottom: 0;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-content a{
    color: {{ settings.link_color }};
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-content a:hover{
    color: {{ settings.link_hover_color }};
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown{
    margin: 30px 0;
    display: flex;
    align-items: center;
    justify-content: center;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-days,
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-hours,
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-minutes,
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-seconds{
    text-align: center;
    margin: 0 15px;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-days .mdp-count,
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-hours .mdp-count,
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-minutes .mdp-count,
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-countdown .mdp-seconds .mdp-count{
    font-size: 38.3904px;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-button{
    margin-top: 30px;
    text-transform: uppercase;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-signup-form{
    margin-top: 30px;
    text-align: left;
    max-width: 400px;
    margin-left: auto;
    margin-right: auto;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-signup-form .mdp-row{
    display: flex;
    align-items: center;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-signup-form .mdp-row input{
    padding: 9px 15px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    border-radius: 0;
    background-image: none !important;
    background-color: white;
    font-size: 14px;
    width: 100%;
    margin-right: 5px;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-signup-form .mdp-row button{
    border-radius: 0;
    background-image: none !important;
    cursor: pointer;
    width: auto;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    padding: 15px 45px;
    font-size: 14px;
    line-height: 19px;
    background-color: #343434;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    border-color: #4c90c7;
    color: white;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-signup-form .mdp-row button:hover{
    background-color: white;
    color: #2a2a2a;
}
.mdp-weare-call-to-action-shop .mdp-data .mdp-footer .mdp-signup-form .mdp-row button::before{
    content: none;
}
.mdp-weare-call-to-action-shop[data-variant=style_1] .mdp-data .mdp-footer .mdp-button{
    border-radius: 0;
    background-image: none !important;
    cursor: pointer;
    width: auto;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    padding: 15px 45px;
    font-size: 14px;
    line-height: 19px;
    background-color: #343434;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    border-color: #4c90c7;
    color: white;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-call-to-action-shop[data-variant=style_1] .mdp-data .mdp-footer .mdp-button:hover{
    background-color: white;
    color: #2a2a2a;
}
.mdp-weare-call-to-action-shop[data-variant=style_1] .mdp-data .mdp-footer .mdp-button::before{
    content: none;
}
.mdp-weare-call-to-action-shop[data-variant=style_2]{
    color: white;
}
.mdp-weare-call-to-action-shop[data-variant=style_2] .mdp-data .mdp-header .mdp-title{
    color: white;
}
.mdp-weare-call-to-action-shop[data-variant=style_2] .mdp-data .mdp-footer .mdp-button{
    padding: 9px 15px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    border-radius: 0;
    background-image: none !important;
    font-size: 14px;
    background-color: white;
    border-color: white;
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-call-to-action-shop[data-variant=style_2] .mdp-data .mdp-footer .mdp-button:hover{
    background-color: #5838e9;
    border-color: #5838e9;
    color: white;
}

