@keyframes mdp_pulse {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.5;
  }
  100% {
    opacity: 1;
  }
}
@keyframes mdp_hotspot_pulse {
  0% {
    transform: scale(1);
    opacity: 1;
  }
  100% {
    transform: scale(1.5);
    opacity: 0;
  }
}
@keyframes mdp_spin {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}
@keyframes mdp_checkmark {
  0% {
    height: 0;
    width: 0;
    opacity: 1;
  }
  20% {
    height: 0;
    width: 7px;
    opacity: 1;
  }
  40% {
    height: 14px;
    width: 7px;
    opacity: 1;
  }
  90% {
    height: 14px;
    width: 7px;
    opacity: 1;
  }
  100% {
    height: 14px;
    width: 7px;
    opacity: 0;
  }
}
@keyframes mdp_checkmark_big {
  0% {
    height: 0;
    width: 0;
    opacity: 1;
  }
  20% {
    height: 0;
    width: 10px;
    opacity: 1;
  }
  40% {
    height: 22px;
    width: 10px;
    opacity: 1;
  }
  90% {
    height: 22px;
    width: 10px;
    opacity: 1;
  }
  100% {
    height: 22px;
    width: 10px;
    opacity: 0;
  }
}
@keyframes mdp_slideout {
  100% {
    width: 0;
  }
}
@keyframes mdp_fadeInUp {
  from {
    opacity: 0;
    transform: translate3d(0, 100%, 0);
  }
  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes mdp_fadeOutDown {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
    transform: translate3d(0, 100%, 0);
  }
}
@keyframes mdp_video_scale {
  0% {
    transform: scale(1);
  }
  100% {
    transform: scale(1.8);
    opacity: 0;
  }
}
