.mdp-weare-hero-3 {
    position: relative;
    z-index: 0;
}
.mdp-weare-hero-3 > .mdp-overlay {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-position: center;
    background-size: cover;
    pointer-events: none;
    z-index: -1;
}
.mdp-weare-hero-3 .mdp-container {
    position: relative;
    text-align: center;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-title {
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    color: {{ settings.mdp_header_color }};
    text-align: center;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-subtitle {
    margin-bottom: 5px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 22px;
    letter-spacing: 0.133333px;
    text-transform: uppercase;
    color: #343434;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-button {
    font-size: 15px;
    letter-spacing: 0.5px;
    display: inline-flex;
    white-space: nowrap;
    align-items: center;
    color: #2a2a2a;
    border-bottom: 1px solid transparent;
    position: relative;
    font-weight: normal;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-button .mdp-text {
    margin-right: 5px;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-button .mdp-icon {
    font-size: 0;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-button .mdp-icon svg {
    width: 20px;
    height: 20px;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-button::before {
    content: "";
    position: absolute;
    transition: 0.3s all ease-in-out;
    left: 0;
    right: 0;
    bottom: -4px;
    height: 0;
    border-bottom: 1px solid #2a2a2a;
    opacity: 0;
    transform: translateY(5px);
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-button:hover::before {
    opacity: 1;
    transform: translateY(0);
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-row-full {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-row-full .mdp-row-left {
    margin-right: 30px;
}
@media (max-width: 600px) {
    .mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-row-full {
        display: block;
    }
    .mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-row-full .mdp-row-left {
        justify-content: center;
        padding: 10px 10px 5px 10px;
        margin-right: 0;
    }
    .mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-row-full .mdp-row-right {
        justify-content: center;
        padding: 5px 10px 10px 10px;
    }
}
.mdp-weare-hero-3 .mdp-container > .mdp-header {
    margin-bottom: 0;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-title {
    margin-bottom: 14px;
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_header_color }};
    text-align: center;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-title:after {
    content: "..";
    display: block;
    margin-top: -10px;
}
.mdp-weare-hero-3 .mdp-container > .mdp-header .mdp-subtitle {
    margin-top: 15px;
}
.mdp-weare-hero-3 .mdp-container .mdp-header {
    width: 50%;
    margin-left: auto;
    margin-right: auto;
}
.mdp-weare-hero-3 .mdp-container .mdp-header .mdp-title {
    font-size: 50px;
}
.mdp-weare-hero-3 .mdp-container .mdp-header .mdp-subtitle {
    margin-top: 0;
    margin-bottom: 15px;
}
.mdp-weare-hero-3 .mdp-container .mdp-data {
    width: 70%;
    margin-left: auto;
    margin-right: auto;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-content p:first-child {
    margin-top: 0;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-content p:last-child {
    margin-bottom: 0;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-content a {
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-content a:hover {
    color: #5838e9;
    transform: scale(1.5);
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items {
    display: flex;
    align-items: start;
    flex-wrap: wrap;
    margin-left: -15px;
    margin-right: -15px;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items:not(:first-child) {
    margin-top: 50px;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items a.mdp-item:hover {
    background-color: white;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item {
    width: calc(33.33% - 30px);
    box-sizing: border-box;
    margin-left: 15px;
    margin-right: 15px;
    margin-bottom: 30px;
    padding: 30px;
    background-color: rgba(0, 0, 0, 0.04);
    transition: 0.3s all ease-in-out;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item .mdp-icon svg {
    width: 40px;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item .mdp-title {
    margin-top: 20px;
    color: {{ settings.mdp_header_color }};
    font-size: 12px;
    text-transform: uppercase;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item .mdp-text {
    margin-top: 15px;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item .mdp-text p:first-child {
    margin-top: 0;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item .mdp-text p:last-child {
    margin-bottom: 0;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item .mdp-text a {
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item .mdp-text a:hover {
    color: #5838e9;
    transform: scale(1.5);
}
@media (max-width: 768px) {
    .mdp-weare-hero-3 .mdp-container .mdp-header {
        width: 100%;
    }
    .mdp-weare-hero-3 .mdp-container .mdp-data {
        width: 100%;
    }
    .mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items {
        display: block;
        margin-left: 0;
        margin-right: 0;
    }
    .mdp-weare-hero-3 .mdp-container .mdp-data .mdp-items .mdp-item {
        width: 100%;
        margin-left: 0;
        margin-right: 0;
    }
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-header .mdp-title {
    color: white;
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-header .mdp-subtitle {
    color: white;
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-data .mdp-content {
    color: white;
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-data .mdp-items a.mdp-item:hover {
    background-color: #2a2a2a;
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-data .mdp-items .mdp-item {
    background-color: rgba(255, 255, 255, 0.1);
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-data .mdp-items .mdp-item .mdp-icon {
    color: white;
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-data .mdp-items .mdp-item .mdp-title {
    color: white;
}
.mdp-weare-hero-3.mdp-inverted .mdp-container .mdp-data .mdp-items .mdp-item .mdp-text {
    color: white;
}