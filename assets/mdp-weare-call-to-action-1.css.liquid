.mdp-weare-call-to-action-1 .mdp-data .mdp-col{
    display: flex;
    align-items: center;
    justify-content: center;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-call-box {
    width: max-content;
    padding: 35px 100px;
    box-sizing: border-box;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-call-box .mdp-header .mdp-title {
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: 48px;
    line-height: {{ settings.mdp_header_line_height }}em;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_header_color }};
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-call-box .mdp-content {
    font-family: "{{ settings.weare_font_text }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_paragraph_font_size }}px;
    line-height: {{ settings.mdp_paragraph_line_height }}em;
    display: flex;
    align-items: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: {{ settings.mdp_paragraph_color }};
    margin-top: 15px;
    margin-bottom: 0;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-call-box .mdp-content p {
    width: 100%;
    text-align: center;
}
.mdp-weare-call-to-action-1{
    text-align: center;
}
.mdp-weare-call-to-action-1 > div{
    position: relative;
    z-index: 0;
    padding-top: 100px;
    padding-bottom: 100px;
}
.mdp-weare-call-to-action-1 > div > .mdp-overlay{
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-position: center;
    background-size: cover;
    pointer-events: none;
    z-index: -1;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col{
    position: relative;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-header .mdp-title{
    font-size: 31.992px;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-content{
    margin-top: 15px;
    font-size: 20px;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-content p:first-child{
    margin-top: 0;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-content p:last-child{
    margin-bottom: 0;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-content a{
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    text-decoration: underline;
    display: inline;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-content a:hover{
    color: #5838e9;
    transform: scale(1.5);
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-footer .mdp-countdown{
    margin: 30px 0;
    display: flex;
    align-items: center;
    justify-content: center;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-footer .mdp-countdown .mdp-days,
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-footer .mdp-countdown .mdp-hours,
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-footer .mdp-countdown .mdp-minutes,
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-footer .mdp-countdown .mdp-seconds{
    text-align: center;
    margin: 0 15px;
}
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-footer .mdp-countdown .mdp-days .mdp-count,
.mdp-weare-call-to-action-1 .mdp-data .mdp-col .mdp-footer .mdp-countdown .mdp-hours .mdp-count,
