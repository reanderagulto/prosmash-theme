.mdp-weare-slideshow-home-11 .mdp-slide {
    position: relative;
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box {
    text-align: left;
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box .mdp-footer .mdp-weare-social-sharing{
    margin-bottom: 30px;
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box .mdp-header .mdp-subtitle {
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: bold;
    font-size: 16px;
    line-height: 22px;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    color: {{ settings.mdp_header_color }};
    margin-top: 40px;
    margin-bottom: 10px;
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box .mdp-header .mdp-title {
    font-family: "{{ settings.weare_font_header }}";
    font-style: normal;
    font-weight: bold;
    font-size: {{ settings.mdp_header_font_size }}px;
    line-height: {{ settings.mdp_header_line_height }}em;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_header_color }};
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box .mdp-content {
    margin-top: 8px;
    margin-bottom: 17px;
    font-family: "{{ settings.weare_font_text }}";
    font-style: normal;
    font-weight: normal;
    font-size: {{ settings.mdp_paragraph_font_size }}px;
    line-height: {{ settings.mdp_paragraph_line_height }}em;
    letter-spacing: 0.4px;
    color: {{ settings.mdp_paragraph_color }};
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box .mdp-footer .mdp-button {
    margin-top: 30px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    border-radius: 0;
    background-image: none !important;
    cursor: pointer;
    width: auto;
    font-weight: bold;
    padding: 15px 45px;
    font-size: 14px;
    line-height: 19px;
    background-color: #343434;
    text-align: center;
    letter-spacing: 0.4px;
    text-transform: uppercase;
    border-color: #4c90c7;
    color: white;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box .mdp-footer .mdp-button:hover {
    background-color: white;
    color: #2a2a2a;
}
.mdp-weare-slideshow-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-content-box .mdp-header .mdp-title {
    color: white;
}
.mdp-weare-slideshow-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-content-box .mdp-content {
    color: white;
}
.mdp-weare-slideshow-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-content-box .mdp-footer .mdp-button {
    padding: 9px 15px;
    font-family: {{ settings.weare_font_text }};
    font-style: normal;
    font-weight: 600;
    border-radius: 0;
    background-image: none !important;
    font-size: 14px;
    background-color: white;
    border-color: white;
    color: #2a2a2a;
    transition: 0.3s all ease-in-out;
    display: inline-block;
    white-space: nowrap;
}
.mdp-weare-slideshow-home-11 .mdp-slide.mdp-inverted .mdp-data .mdp-content-box .mdp-footer .mdp-button:hover {
    background-color: #5838e9;
    border-color: #5838e9;
    color: white;
}

.mdp-weare-slideshow-home-11 .mdp-slide .mdp-data .mdp-content-box .mdp-footer .mdp-box-img img{
    min-width: 100%;
}
.mdp-weare-slideshow-home-11 .mdp-slide .mdp-media img{
    width: 100%;
}
.swiper-button-prev-slideshow-home-11:after,
.swiper-button-next-slideshow-home-11:after{
    color: white;
}

.swiper-button-prev-slideshow-home-11:hover:after,
.swiper-button-next-slideshow-home-11:hover:after{
    color: #E9384F;
}

